  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "db:generate": "prisma generate --schema=src/core/database/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=src/core/database/schema.prisma",
    "db:push": "prisma db push --schema=src/core/database/schema.prisma",
    "db:setup": "pnpm db:migrate && pnpm db:generate",
    "db:studio": "prisma studio --schema=src/core/database/schema.prisma",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"{src,apps,libs,test}/**/*.{ts,js,json,md,yml,yaml}\" && prisma format --schema=src/core/database/schema.prisma"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
